package br.com.logica.programacao.data.retroativa;
import java.util.Scanner;


public class Data {
	
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		DataRetroativa dataRetroativa = new DataRetroativa();
		SeparaData cd = new SeparaData();
		System.out.println ("Digite a data a qual desejar");
		cd.data = scanner.nextLine();
		System.out.println ("Dia retroativo"+dataRetroativa.diaAnterior+dataRetroativa.mesAnterior+dataRetroativa.anoAnterior);;
		
	}
		
		
}
